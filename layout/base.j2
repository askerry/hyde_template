<!doctype html>
<html lang="en">
    <head>
        <link href="http://fonts.googleapis.com/css?family=Anton"
            rel="stylesheet" type="text/css">
            <title>AES hyde template {% if resource.meta.title-%}{{ resource.meta.title}}{%-endif %}</title>
        <meta charset="utf-8" />
        <link rel="stylesheet" href="{{ media_url('css/style.less') }}">

        <meta charset="utf-8">
        <title>Website Title {% if resource.meta.title-%}{{ resource.meta.title}}{%-endif %}</title>
        <meta name="description" content="cdescription string">
        <meta name="keywords" content="keyword, strings">
        <meta name="author" content="Amy E. Skerry">
          <meta name="viewport" content="width=device-width">
        <!--Bootstrap-->    
        <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
        <!--JQUERY-->
        <script language="JavaScript" src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
        <!--my less-->
        <link rel="stylesheet/less" type="text/less" href="{{ media_url('css/style.less') }}" />
        <script src="{{ media_url('js/less-1.6.0.min.js') }}" type="text/javascript"></script><!--dev only-->
        <!-- Latest compiled and minified bootstrap JavaScript (needed for bootbox)-->
        <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js"></script> 
        <!--Less.js -->
        <script src="//cdnjs.cloudflare.com/ajax/libs/less.js/2.5.0/less.min.js"></script>
        <link href="{{ media_url('bootstrap/bootstrap.min.css') }}" rel="stylesheet">
          
    </head>
    <body>
    <div id="page">
        <div id="title" class="row">
            <div id="textdiv">
              <h1> {% if resource.meta.title-%}{{ resource.meta.title}}{%-endif %}</h1>
              <p>tag line</p>
            </div>
        </div>
        <nav>
              {% import "macros.j2" as macros with context %}
              {{ macros.render_basic_menu() }}
        </nav>
        <div id="container" class="row">
          <div id="content" class="col-md-9">
            {% filter markdown -%}
            {% block content %}{% endblock %}
            {%- endfilter %}
          </div>
          <div id="sidebar" class="col-md-3">
             <img class="logo" src="{{ media_url('img/logo.jpg') }}" alt="logo">
          </div>
        </div>
    </div>
    </body>
</html>
